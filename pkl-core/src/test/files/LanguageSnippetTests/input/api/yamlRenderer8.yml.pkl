// Test that non-String keys can be rendered as YAML map keys.
mappingInt = new Mapping {
  [5] = "abc"
  [10] = "def"
  [20] = "ghi"
}

mapInt = mappingInt.toMap()

mappingFloat = new Mapping {
  [5.0] = "abc"
  [10.5] = "def"
  [20.999999990] = "ghi"
}

mapFloat = mappingFloat.toMap()

mappingBool = new Mapping {
  [true] = "abc"
  [false] = "def"
}

mapBool = mappingBool.toMap()

mappingNull = new Mapping {
  [null] = "abc"
}

mapNull = mappingNull.toMap()

mappingOfListing = new Mapping {
  [new Listing { 1; 2; 3 }] = "abc"
}

mapOfListing = mappingOfListing.toMap()

mappingOfMapping = new Mapping {
  [new Mapping { ["one"] = "one"}] = "abc"
}

mapOfMapping = mappingOfMapping.toMap()

mappingOfNestedObject = new Mapping {
  [new Dynamic { foo { bar { 1 } } }] = "abc"
}

output {
  renderer = new YamlRenderer {}
}
